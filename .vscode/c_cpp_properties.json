{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/includes",
                "${workspaceFolder}/includes/raw_packet"
            ],
            "defines": [
                "_GNU_SOURCE",
                "__USE_MISC"
            ],
            "compilerPath": "/usr/bin/clang",
            "cppStandard": "c++14",
            "cStandard": "c99",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-Werror",
                "-Wall",
                "-Wextra"
            ]
        }
    ],
    "version": 4
}